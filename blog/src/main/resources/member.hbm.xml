<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.project.vo">
	<class name="com.project.vo.MemberVO" table="BLOG_MEMBER">
        <id name="idx" column="idx">
        </id>
        <property name="id" column="id" />
        <property name="password" column="password" />
        <property name="email" column="email" />
        <property name="reg_id" column="reg_id" />
        <property name="reg_date" column="reg_date" />
        <property name="mod_id" column="mod_id" />
        <property name="mod_date" column="mod_date" />
    </class> 
</hibernate-mapping>